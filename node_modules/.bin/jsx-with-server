#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/bin/jsx-with-server/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/jsx/bin/jsx-with-server/node_modules:/data/node_modules/jsx/bin/node_modules:/data/node_modules/jsx/node_modules" "$basedir/node"  "$basedir/../jsx/bin/jsx-with-server" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/bin/jsx-with-server/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules/jsx/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/0.9.89/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/jsx/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/jsx/bin/jsx-with-server/node_modules:/data/node_modules/jsx/bin/node_modules:/data/node_modules/jsx/node_modules" node  "$basedir/../jsx/bin/jsx-with-server" "$@"
  ret=$?
fi
exit $ret
