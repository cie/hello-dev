#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/bin/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/brunch/bin/brunch/node_modules:/data/node_modules/brunch/bin/node_modules:/data/node_modules/brunch/node_modules" "$basedir/node"  "$basedir/../brunch/bin/brunch" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/bin/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/2.10.17/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/brunch/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/brunch/bin/brunch/node_modules:/data/node_modules/brunch/bin/node_modules:/data/node_modules/brunch/node_modules" node  "$basedir/../brunch/bin/brunch" "$@"
  ret=$?
fi
exit $ret
