#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/bin/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../ncp/2.0.0/node_modules/ncp/bin/ncp" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/bin/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/2.0.0/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/ncp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" node  "$basedir/../../../../../../ncp/2.0.0/node_modules/ncp/bin/ncp" "$@"
  ret=$?
fi
exit $ret
