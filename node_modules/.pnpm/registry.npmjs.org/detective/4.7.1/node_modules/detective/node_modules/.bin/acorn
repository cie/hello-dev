#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/bin/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../acorn/5.7.3/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/bin/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/5.7.3/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" node  "$basedir/../../../../../../acorn/5.7.3/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
