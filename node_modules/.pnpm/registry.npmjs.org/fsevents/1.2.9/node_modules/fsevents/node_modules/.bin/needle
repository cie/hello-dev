#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/node_modules:/home/cie/git/hello-dev/node_modules:/home/cie/git/node_modules:/home/cie/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
  ret=$?
else 
  NODE_PATH="/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/cie/git/hello-dev/node_modules/.pnpm/node_modules:/home/cie/git/hello-dev/node_modules:/home/cie/git/node_modules:/home/cie/node_modules:/home/node_modules:/node_modules" node  "$basedir/../needle/bin/needle" "$@"
  ret=$?
fi
exit $ret
