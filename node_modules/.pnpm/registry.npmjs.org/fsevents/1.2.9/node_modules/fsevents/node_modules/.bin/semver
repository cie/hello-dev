#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/bin/semver/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/bin/semver/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/semver/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules" node  "$basedir/../semver/bin/semver" "$@"
  ret=$?
fi
exit $ret
