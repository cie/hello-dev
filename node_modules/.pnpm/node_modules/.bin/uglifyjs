#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/bin/uglifyjs/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/bin/uglifyjs/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/bin/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/node_modules" "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/bin/uglifyjs/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules/uglify-js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/2.6.4/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/uglify-js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/bin/uglifyjs/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/bin/node_modules:/data/node_modules/.pnpm/node_modules/uglify-js/node_modules" node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret
