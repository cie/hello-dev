#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/esvalidate.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/esprima/bin/esvalidate.js/node_modules:/data/node_modules/.pnpm/node_modules/esprima/bin/node_modules:/data/node_modules/.pnpm/node_modules/esprima/node_modules" "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/esvalidate.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules/esprima/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/1.0.4/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/esprima/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/esprima/bin/esvalidate.js/node_modules:/data/node_modules/.pnpm/node_modules/esprima/bin/node_modules:/data/node_modules/.pnpm/node_modules/esprima/node_modules" node  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
