#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/bin/cake/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/bin/cake/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/bin/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/node_modules" "$basedir/node"  "$basedir/../coffeescript/bin/cake" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/bin/cake/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules/coffeescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/1.12.7/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/coffeescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/bin/cake/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/bin/node_modules:/data/node_modules/.pnpm/node_modules/coffeescript/node_modules" node  "$basedir/../coffeescript/bin/cake" "$@"
  ret=$?
fi
exit $ret
