#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/cli.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/mime/cli.js/node_modules:/data/node_modules/.pnpm/node_modules/mime/node_modules" "$basedir/node"  "$basedir/../mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/cli.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/mime/cli.js/node_modules:/data/node_modules/.pnpm/node_modules/mime/node_modules" node  "$basedir/../mime/cli.js" "$@"
  ret=$?
fi
exit $ret
