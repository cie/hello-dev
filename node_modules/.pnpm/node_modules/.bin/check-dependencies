#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/bin/cli.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/bin/cli.js/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/bin/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/node_modules" "$basedir/node"  "$basedir/../check-dependencies/bin/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/bin/cli.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules/check-dependencies/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/1.0.1/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/check-dependencies/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/bin/cli.js/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/bin/node_modules:/data/node_modules/.pnpm/node_modules/check-dependencies/node_modules" node  "$basedir/../check-dependencies/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
