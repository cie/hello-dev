#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/bin/cmd.js/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/bin/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/node_modules" "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/0.5.1/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/mkdirp/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/bin/cmd.js/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/bin/node_modules:/data/node_modules/.pnpm/node_modules/mkdirp/node_modules" node  "$basedir/../mkdirp/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
