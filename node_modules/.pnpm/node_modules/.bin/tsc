#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/bin/tsc/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/typescript/bin/tsc/node_modules:/data/node_modules/.pnpm/node_modules/typescript/bin/node_modules:/data/node_modules/.pnpm/node_modules/typescript/node_modules" "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
  ret=$?
else 
  NODE_PATH="/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/bin/tsc/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/bin/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules/typescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/2.1.6/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/data/node_modules/.pnpm/registry.npmjs.org/node_modules:/data/node_modules/.pnpm/node_modules:/data/node_modules:/node_modules:/data/node_modules/.pnpm/node_modules/typescript/bin/tsc/node_modules:/data/node_modules/.pnpm/node_modules/typescript/bin/node_modules:/data/node_modules/.pnpm/node_modules/typescript/node_modules" node  "$basedir/../typescript/bin/tsc" "$@"
  ret=$?
fi
exit $ret
